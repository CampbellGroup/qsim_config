class arduino_dac_config(object):
    '''
    configuration file for arduino switch client
    info is the configuration dictionary in the form
    {channel_name: (port, display_location, inverted)), }
    '''
    info = {'DAC 1': ('DAC1: Pin 1', 1),
            'DAC 2': ('DAC2: Pin 2', 2),
	    'DAC 3': ('DAC3: Pin 4', 3),
	    'DAC 4': ('DAC4: Pin 5', 4),
	    'DAC 5': ('DAC5: Pin 6', 5),
            'DAC 6': ('DAC6: Pin 7', 6),
	    'DAC 7': ('DAC7: Pin 11', 7),
	    'DAC 8': ('DAC8: Pin 12', 8),
            }
